# CHURCHOS Frontend Environment Variables
# Sacred Operating System for Prophetic Church Governance

# API Configuration
REACT_APP_SITE_URL=https://scrollchurchos.xyz
CORS_ORIGINS=https://scrollchurchos.xyz
REACT_APP_API_VERSION=v1

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_firebase_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# OpenAI Configuration
REACT_APP_OPENAI_API_KEY=your_openai_api_key
REACT_APP_OPENAI_MODEL=gpt-4

# Stripe Configuration
REACT_APP_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
REACT_APP_STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=G-KS1J3K7P37
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token

# Feature Flags
REACT_APP_ENABLE_AI_CHARACTERS=true
REACT_APP_ENABLE_LIVESTREAMING=true
REACT_APP_ENABLE_XR_HOLYLAND=true
REACT_APP_ENABLE_MOBILE_CONTROL=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_BILLING=true

# Development
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=true 